.jianli{
  position: absolute;
  width: 198rpx;
  height: 67rpx;
  top: 16rpx;
  left: 30rpx;
  font-size: 28rpx;
  color: #799DF1;
  letter-spacing: 1.46rpx;
  display:flex;
  align-items: center;
  justify-content: center;
}
.jianli image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 16rpx;
}
.tongzhi{
  position: absolute;
  top: 10rpx;
  right: 40rpx;
  width: 80rpx;
  height: 80rpx;
}
.tongzhi image{
  display: block;
  width: 80rpx;
  height: 80rpx;
}
.tongzhi view{
  position: absolute;
  top: 0;
  right: 0;
  padding: 0 10rpx;
  height: 34rpx;
  background: #E2442D;
  border-radius: 17rpx;
  font-size: 24rpx;
  color: #FFFFFF;
}
.wode_list{
  width: 100%;
  height: 100%;
  background: #fff;
}
.logo{
  display: block;
  width: 132rpx;
  height: 132rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  border-radius: 50%;
  margin-bottom: 20rpx;
}
.vip_d{
  position: absolute;
  width: 140rpx;
  height: 151rpx;
  top: 36rpx;
  left: 50%;
  margin-left: -70rpx;
}
.logo_text{
  display: block;
  text-align: center;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 1.67rpx;
}
.vip{
  height: 105rpx;
  font-size: 32rpx;
  color: #9F6533;
  line-height: 105rpx;
}
.vip .zuanshi{
  width: 40rpx;
  height: 36rpx;
  margin-left: 40rpx;
  margin-right: 34rpx;
  margin-top: 35rpx;
}
.vip .right{
  width: 27rpx;
  height: 27rpx;
  float: right;
  margin-top: 40rpx;
  margin-right: 40rpx;
}
.wode_btn{
  width: 260rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #FFFFFF;
  background: #799DF1;
  border-radius: 8rpx;
  margin: 0 auto;
  margin-top: 30rpx;
}
.wode_btn3{
  width: 260rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 26rpx;
  color: #FFFFFF;
  background: #ccc;
  border-radius: 8rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}
.wode_bor{
  width: 100%;
  height: 20rpx;
  background: #EBEBEB;
  margin-top: 30rpx;
}
.list view{
  width: 670rpx;
  height: 106rpx;
  border-bottom: 1rpx solid #CCCCCC;
  margin: 0 auto;
}
.list image{
  float: left;
  width: 40rpx;
  height: 40rpx;
  margin-right: 34rpx;
  margin-top: 35rpx;
}
.list text{
  float: left;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 1.67rpx;
  margin-top: 30rpx;
}
.fenxiang{
  position: absolute;
  width: 100%;
  left: 0;
  top: 0;
  z-index: 2;
  height: 115rpx;
  opacity: 0;
}
.shibai_tishi{
  font-size: 28rpx;
  color: #E2442D;
  margin-top: 20rpx;
  line-height: 34rpx;
  display:flex;
  align-items: center;
  justify-content: center;
}
.shibai_tishi image{
  width: 34rpx;
  height: 34rpx;
  margin-right: 20rpx;
}
.list .right{
  width: 27rpx;
  height: 27rpx;
  float: right;
  margin-right: 0rpx;
}
.biaozhun{
  width: 430rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  text-align: center;
  color: #799DF1;
  margin: 0 auto;
  margin-top: 20rpx;
  text-decoration:underline;
  margin-bottom: 180rpx;
}
