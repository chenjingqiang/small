/* pages/uear/uear.wxss */
page{
  height: 100%;
  background: #f5f5f5;
}
.shanyi{
  height: 100%;
}
.fixed{
  position: fixed;
  width: 100%;
  height: 221rpx;
  bottom: 0;
  left: 0;
  background: #fff;
  z-index: 2;
  padding-left: 40rpx;
}
.fixed .fu,.fixed .fu2{
  display: flex;
  align-content: center;
}
.fixed .fu view{
  width: 310rpx;
  height: 96rpx;
  line-height: 96rpx;
  background: #799DF1;
  border-radius: 71rpx;
  font-size: 36rpx;
  color: #FFFFFF;
  letter-spacing: 0.16rpx;
  text-align: center;
  margin-top: 40rpx;
}
.fixed .fu .china{
  margin-left: 50rpx;
  background: #E95B5A;
}
.fixed .fu2 view{
  width: 310rpx;
  text-align: center;
  font-size: 24rpx;
  color: #BBBBBB;
  letter-spacing: 0.11rpx;
  margin-top: 12rpx;
}
.fixed .fu2 .china{
  margin-left: 50rpx;
}
.kong{
   position: fixed;
   top: 50%;
   left: 50%;
   width: 302rpx;
   height: 363rpx;
   margin-left: -151rpx;
   margin-top: -280rpx;
}
.list_fu{
  width: 100%;
  padding-top: 36rpx;
  padding-bottom: 216rpx;
  background: #f5f5f5;
}
.list{
  max-width: 576rpx;
  min-width: 84rpx;
  box-shadow: -1rpx 4rpx 7rpx 1rpx #E8E8E8;
  padding: 0 30rpx;
  padding-top: 14rpx;
  padding-bottom: 34rpx;
  background: #FFFFFF;
  margin-bottom: 24rpx;
}
.list .top{
  max-width: 476rpx;
  min-width: 84rpx;
  word-wrap:break-word;
  font-size: 30rpx;
  color: #999999;
  letter-spacing: 0.13rpx;
  padding: 20rpx 0;
  text-align: justify;
}
.list .bottom{
  text-align: justify;
  border-top: 1px solid #ccc;
  padding-top: 20rpx;
  position: relative;
  max-width: 476rpx;
  min-width: 84rpx;
  word-wrap:break-word;
  font-size: 30rpx;
  color: #333333;
  letter-spacing: 0.13rpx;
  padding-right: 40rpx;
}
.yinliang{
  width: 30rpx;
  height: 27rpx;
  position: absolute;
  right: 0;
  bottom: 0;
}
.fl2{
  float: left;
  border-radius: 20rpx 20rpx 20rpx 0;
  margin-left: 33rpx;
}
.fr2{
  float: right;
  border-radius: 20rpx 20rpx 0 20rpx;
  margin-right: 33rpx;
}
.ting{
  width: 100%;
  height: 200rpx;
}
.box{
  position: fixed;
  z-index: 5;
  left: 50%;
  top: 30%;
  margin-left: -245rpx;
  width: 430rpx;
  background: rgba(0,0,0,0.65);
  border-radius: 10px;
  padding: 40rpx 30rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  letter-spacing: 0.11rpx;
  text-align: center;
  word-wrap:break-word;
}
.box .tit{
  width: 100%;
  font-size: 32rpx;
  line-height: 40rpx;
  letter-spacing: 1rpx;
  text-align: justify;
  height: 80rpx;
}
.box image{
  display: block;
  width: 50rpx;
  height: 76rpx;
  margin: 0 auto;
  margin-top: 38rpx;
  margin-bottom: 20rpx;
}



